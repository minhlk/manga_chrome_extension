{
    "manifest_version": 2,
    "name": "MK Manga",
    "description": "Keep up with latest ecchi manga",
    "version": "1",
    "author": "MKProduction",
    "background": {
        "scripts": [
            "jquery-3.3.1.slim.js",
            "background.js"
            
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Good day to watch ecchi",
        "default_popup" : "index.html"
    },
    "permissions": [
        "activeTab",
        "notifications"
    ]
}